using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Admissions.Utilities
{
    internal class BooleanStringBinding
    {
        internal BooleanStringBinding(bool id, string description)
        {
            this.id = id;
            this.description = description;
        }

        bool id;

        public bool Id
        {
            get { return id; }
            set { id = value; }
        }

        string description;

        public string Description
        {
            get { return description; }
            set { description = value; }
        }
    }
}
